<%- include ('./dash_include/main_header.ejs') %>
<%- include ('./dash_include/HEADER_GEN.ejs') %>

  <main id="main">
<% if(service) {%>
    <!-- ======= Intro Single ======= -->
    <section class="intro-single">
      <div class="container">
        <div class="row">
          <div class="col-md-12 col-lg-8">
            <div class="title-single-box">
              <h1 class="title-single"><%= service.heading %></h1>
            </div>
          </div>
          <div class="col-md-12 col-lg-4">
            <nav aria-label="breadcrumb" class="breadcrumb-box d-flex justify-content-lg-end">
              <ol class="breadcrumb">
                <li class="breadcrumb-item">
                  <a href="/homing">Home</a>
                </li>
                <li class="breadcrumb-item active" aria-current="page">
                  About
                </li>
              </ol>
            </nav>
          </div>
        </div>
      </div>
    </section><!-- End Intro Single-->

    <!-- ======= About Section ======= -->
    <section class="section-about">
      <div class="container">
        <div class="row">
          <div class="col-sm-12 position-relative">
            <div class="about-img-box">
              <img src="data:Binary?<%= service.img.contentType %>;base64,<%= service.img.data.toString('base64') %>" alt="" class="img-fluid">
            </div>
            <div class="sinse-box">
              <h3 class="sinse-title">Valiantfoot Limited
                <span></span>
                <br> Since 2017
              </h3>
              <p>Housing & Property</p>
            </div>
          </div>
          <div class="col-md-12 section-t8 position-relative">
            <div class="row">
              <div class="col-md-6 col-lg-5">
                <img src="/img/about-2.jpg" alt="" class="img-fluid">
              </div>
              <div class="col-lg-2  d-none d-lg-block position-relative">
                <div class="title-vertical d-flex justify-content-start">
                  <span>Valiantfoot Exclusive Property</span>
                </div>
              </div>
              <div class="col-md-6 col-lg-5 section-md-t3 post-content ">
                <div class="title-box-d">
                  <h3 class="title-d">En savoir 
                    <span class="color-d">plus</span> sur
                    <br> nous
                  </h3>
                </div>
                <p class="color-text-a post-intro">
                 <%= service.about %>
                </p>
               
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
<% } %>
    <!-- =======Team Section ======= -->
   

  </main><!-- End #main -->
<%- include ('./dash_include/FOOTER_GEN.ejs') %>
<%- include ('./dash_include/main_footer.ejs') %>